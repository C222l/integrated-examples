{
  "log": {
    "error": "/var/log/v2ray/error.log",
    "access": "/var/log/v2ray/access.log",
    "loglevel": "info"
  },
  "inbounds": [
    {
      "port": 2053, //监听端口
      "protocol": "vless",
      "settings": {
        "clients": [
          {
            "email": "2053@gmail.com",
            "id": "0a652466-dd56-11e9-aa37-5600024c1d6a" //修改成自己的UUID
          }
        ],
        "decryption": "none"
      },
      "streamSettings": {
        "security": "none",
        "network": "kcp",
        "kcpSettings": {
          "congestion": true,
          "seed": "60VoqhfjP79nBQyU" //seed密码，修改成自己的。
        }
      },
      "sniffing": {
        "enabled": true,
        "destOverride": [
          "http",
          "tls"
        ]
      }
    }
  ],
  "outbounds": [
    {
      "tag": "direct",
      "protocol": "freedom",
      "settings": {}
    },
    {
      "tag": "blocked",
      "protocol": "blackhole",
      "settings": {}
    }
  ],
  "routing": {
    "domainStrategy": "AsIs",
    "rules": [
      {
        "type": "field",
        "protocol": [
          "bittorrent"
        ],
        "outboundTag": "blocked"
      }
    ]
  }
}
