{
  "log": {
    "error": "/var/log/v2ray/error.log",
    "access": "/var/log/v2ray/access.log",
    "loglevel": "warning"
  },
  "inbounds": [
    {
      "port": 443, //监听端口
      "protocol": "trojan",
      "settings": {
        "clients": [
          {
            "password":"diy443", //修改成自己密码
            "flow": "xtls-rprx-direct", //启用xtls，必须增加此条参数，参数可xtls-rprx-origin或xtls-rprx-direct可选；否则删除。另支持的v2ray版本应不小于v4.31.3。
            "email": "443@gmail.com"
          }
        ],
        "fallbacks": [
          {
            "dest": "/dev/shm/h1.sock", //http/1.1回落进程
            "xver": 1 //开启PROXY protocol发送，发送请求的真实来源IP和端口给nginx。1或2表示PROXY protocol插件版本。
          },
          {
            "path": "/9ALdGZ9k", //通过路径分流出ws
            "dest": 2007, //分流后转发到2007端口
            "xver": 1 //开启PROXY protocol发送，发送请求的真实来源IP和端口给如下ws应用。1或2表示PROXY protocol插件版本。
          }
        ]
      },
      "streamSettings": {
        "network": "tcp",
        "security": "xtls", //如启用xtls，tls必须改成xtls。
        "xtlsSettings": { //如启用xtls，tlsSettings必须改成xtlsSettings。
          "alpn":[
            "http/1.1" //启用http/1.1连接，web回落也需配置支持http/1.1回落；否则不一致（裸奔），容易被墙探测出，从而被封。
          ],
          "certificates": [
            {
              "certificateFile": "/etc/ssl/tls/xx.yy.crt", //换成你的证书，绝对路径。
              "keyFile": "/etc/ssl/tls/xx.yy.key" //换成你的私钥，绝对路径。
            }
          ]
        }
      },
      "sniffing": {
        "enabled": true,
        "destOverride": [
          "http",
          "tls"
        ]
      }
    },
    {
      "listen": "127.0.0.1", //只监听本机，避免本机外的机器探测到下面端口。
      "port": 2007, // 监听端口
      "protocol": "trojan",
      "settings": {
        "clients": [
          {
            "password":"diy2007", //修改成自己密码
            "email": "2007@gmail.com"
          }
        ]
      },
      "streamSettings": {
        "network": "ws",
        "security": "none",
        "wsSettings": {
          "acceptProxyProtocol": true, //开启PROXY protocol 接收，接收trojan+tcp分流前请求的真实来源IP和端口。
          "path": "/9ALdGZ9k" //修改成自己的path
        }
      },
      "sniffing": {
        "enabled": true,
        "destOverride": [
          "http",
          "tls"
        ]
      }
    }
  ],
  "routing": {
    "domainStrategy": "IPIfNonMatch",
    "rules": [
      {
        "type": "field",
        "protocol": [
          "bittorrent"
        ],
        "outboundTag": "blocked"
      }
    ]
  },
  "outbounds": [
    {
      "protocol": "freedom",
      "settings": {}
    },
    {
      "tag": "blocked",
      "protocol": "blackhole",
      "settings": {}
    }
  ]
}
